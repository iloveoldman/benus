#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules/ts-patch/bin/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules/ts-patch/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules/ts-patch/bin/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules/ts-patch/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/ts-patch@3.3.0/node_modules:/home/runner/workspace/Equicord/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-patch/bin/tspc.js" "$@"
else
  exec node  "$basedir/../ts-patch/bin/tspc.js" "$@"
fi
